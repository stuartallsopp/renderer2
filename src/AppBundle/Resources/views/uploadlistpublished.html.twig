{% if list|length!=0 %}
    <table class="table table-sm">
        <thead>
        <tr>
            <th>Render</th>
            <th class="text-right">Start</th>
            <th class="text-right">End</th>
            <th class="text-right">Actions</th>
        </tr>
        </thead>
        <tbody>
        {% for item in list %}
            <tr>
                <td><a href="#" onclick="expandcollapsesession({{ item.id }},'published','pub_session_list_',this)"><i class="fa fa-plus-square"></i></a> {{ item.filename }} <span class="badge badge-pill badge-success">{{ (item.filesize/1024)|number_format(0, '.', ',') }}Mb</span></td>
                <td class="text-right">{{ item.framestart }}</td>
                <td class="text-right">{{ item.frameend }}</td>
                <td class="text-right">
                    <div class="btn-group btn-group-sm">
                        <button type="button" class="btn btn-secondary" onclick="sendBlendStart({{ item.id }},'upload_list','upload')"><i class="fa fa-cogs"></i></button>
                        <button type="button" class="btn btn-secondary dropdown-toggle dropdown-toggle-split" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                            <span class="sr-only">Toggle Dropdown</span>
                        </button>
                        <div class="dropdown-menu dropdown-menu-right">
                            <a class="dropdown-item" onclick="sendBlendStart({{ item.id }},'upload_list','upload')" href="#"><i class="fa fa-cogs"></i> Render</a>
                            <a class="dropdown-item" onclick="deleteupload({{ item.id }})" href="#"><i class="fa fa-trash"></i> Delete</a>
                        </div>
                    </div>
                </td>
            </tr>
            <tr><td colspan="4" style="padding:0;" id="pub_session_list_{{ item.id }}"></td></tr>
        {% endfor %}
        </tbody>
    </table>
{% else %}
    <div class="card-block">
        <p>No Files Published</p>
    </div>
{% endif %}