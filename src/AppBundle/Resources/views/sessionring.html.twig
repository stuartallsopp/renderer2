<table class="table table-sm">
    <tbody>
    {% for item in queue.sessions %}
        <tr>
            <td></td>
            <td><a href="#" data-toggle="tooltip" data-placement="top" title="{{ item.sessionkey }}">
                    <i class="fa fa-key"></i>
                </a> {{ item.nodename }}</td>
            <td>
                {% if item.framesgot==0 %}
                <span class="badge badge-pill badge-danger">0/{{ item.framestotal }}</span>
                {% elseif item.framesgot==item.framestotal %}
                <span class="badge badge-pill badge-success">{{ item.framestotal }}/{{ item.framestotal }}</span>
                {% else %}
                <span class="badge badge-pill badge-success">{{ item.framesgot }}/{{ item.framestotal }}</span>
                {% endif %}
            </td>
            <td><span class="time" data-time="{{ item.startdatetimer }}"></span></td>
            <td>{% if item.enddatetime %}<span class="time" data-time="{{ item.enddatetimer }}"></span>{% endif %}</td>
            <td class="text-right">
                <div class="btn-group btn-group-sm">
                    <button type="button" class="btn btn-secondary" onclick="sendBlendStart({{ item.id }},'upload_list','session')"><i class="fa fa-cogs"></i></button>
                    <button type="button" class="btn btn-secondary dropdown-toggle dropdown-toggle-split" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                        <span class="sr-only">Toggle Dropdown</span>
                    </button>
                    <div class="dropdown-menu dropdown-menu-right">
                        <a class="dropdown-item" onclick="sendBlendStart({{ item.id }},'upload_list','session')" href="#"><i class="fa fa-cogs"></i> Render</a>
                    </div>
                </div>
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>
<script>
    $(function () {
        $('[data-toggle="tooltip"]').tooltip();
        $('.time').each(function(i,obj){
           $(obj).html(moment($(obj).attr('data-time')).fromNow());
        });

    })
</script>
